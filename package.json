{
  "name": "angular-google-maps",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:agm": "ng build core --configuration production && ng build markerclusterer --configuration production ",
    "build:agm:prod": "ng build core --configuration production && ng build markerclusterer --configuration production ",
    "clean": "rimraf dist node_modules/@agm",
    "prebuild": "yarn clean",
    "move": "npm run clean ; npm run build:agm:prod  ;  mkdir -p node_modules\\@agm\\core ; robocopy  dist\\core node_modules\\@agm\\core",
    "ci": "yarn build:agm:prod "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.6",
    "@angular/common": "^17.3.6",
    "@angular/compiler": "^17.3.6",
    "@angular/core": "^17.3.6",
    "@angular/forms": "^17.3.6",
    "@angular/platform-browser": "^17.3.6",
    "@angular/platform-browser-dynamic": "^17.3.6",
    "@angular/router": "^17.3.6",
    "rxjs": "~7.5.0",
    "snazzy-info-window": "^1.1.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.6",
    "@angular/cli": "~17.3.6",
    "@angular/compiler-cli": "^17.3.6",
    "@compodoc/compodoc": "^1.1.10",
    "@google/markerclustererplus": "^5.0.4",
    "@types/googlemaps": "^3.39.13",
    "@types/jasmine": "~4.0.0",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.22",
    "codelyzer": "^6.0.0",
    "conventional-changelog": "^3.1.21",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^17.3.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.0",
    "typescript": "~5.4.5",
    "yarn": "^1.22.4"
  }
}
